<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}ICT Careers{% endblock %}</title>     
         {% block stylesheets %}     
           {% stylesheets
           '@myBundleictBundle/Resources/views/Default/bootstrap/css/bootstrap.min.css'
           '@myBundleictBundle/Resources/views/Default/font-awesome/css/font-awesome.min.css'
           '@myBundleictBundle/Resources/views/Default/css/local.css'
           'http://www.shieldui.com/shared/components/latest/css/shieldui-all.min.css'
           'http://www.shieldui.com/shared/components/latest/css/light-bootstrap/all.min.css'
           %}
        		<link href="{{ asset_url }}" rel="stylesheet" media="screen" />
    		{% endstylesheets %}
    	 {% endblock %}
        	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
      <div id="wrapper">
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="index.html">ICT Careers</a>
            </div>
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li><a href="#">Student</a></li>
                    <li><a href="{{ path('teacher_skill_main') }}">Skill&Improvement</a></li>
                    <li><a href="{{ path('activity_main') }}">Activities</a></li>
                    <li><a href="{{ path('career_main') }}">Careers</a></li>
                    <li><a href="{{ path('scholarship_main') }}">Scholarships</a></li>
                    <li><a href="#">Advisors</a></li>					
                </ul>
                <ul class="nav navbar-nav navbar-right navbar-user">
				    <li class="active"><a href="{{ path('welcome') }}">Home</a></li>
					<li><a href="#about">About</a></li>
					<li><a href="#contact">Contact</a></li>
                </ul>
            </div>
        </nav>
        {% block body %}{% endblock %}

    </div>

        {% block javascripts %}
        	{% javascripts
    			'@myBundleictBundle/Resources/views/Default/js/jquery-1.10.2.min.js'
    			'@myBundleictBundle/Resources/views/Default/bootstrap/js/bootstrap.min.js'
    			'http://www.shieldui.com/shared/components/latest/js/shieldui-all.min.js'
    			'http://www.prepbootstrap.com/Content/js/gridData.js'    			
			%}
    			<script type="text/javascript" src="{{ asset_url }}"></script>
			{% endjavascripts %}
        {% endblock %}
    </body>
</html>
